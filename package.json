{
	"name": "chrome-session-quicksave",
	"description": "A Chrome extension for quickly saving all open tabs inside a window into the browser bookmarks (with some configurability)",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"start": "snowpack dev",
		"build": "snowpack build",
		"build:watch": "snowpack build --watch",
		"build:clean": "snowpack build --clean",
		"zip:dist": "bestzip release.zip dist/*",
		"pack-release": "run-s build:clean zip:dist"
	},
	"dependencies": {
		"@bjesuiter/serializr-helpers": "^2.2.1",
		"@datorama/akita": "^6.2.4",
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "4.0.0-alpha.57",
		"date-fns": "^2.17.0",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"serializr": "^2.0.5"
	},
	"devDependencies": {
		"@snowpack/plugin-build-script": "^2.1.0",
		"@snowpack/plugin-react-refresh": "2.4.0",
		"@snowpack/plugin-typescript": "^1.2.1",
		"@types/chrome": "0.0.129",
		"@types/jest": "^26.0.20",
		"@types/react": "^17.0.1",
		"@types/react-dom": "^17.0.0",
		"@types/snowpack-env": "^2.3.3",
		"bestzip": "^2.1.7",
		"chokidar-cli": "^2.1.0",
		"esm": "^3.2.25",
		"jest": "^26.6.3",
		"load-json-file": "^6.2.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"snowpack": "^3.0.11",
		"tslib": "^2.1.0",
		"typescript": "^4.1.3",
		"write-json-file": "^4.3.0"
	},
	"license": "MIT"
}
